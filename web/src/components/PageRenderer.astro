---
import Hero from "./blocks/Hero.astro";
import TextBlock from "./blocks/TextBlock.astro";

const { blocks } = Astro.props;
---

{blocks?.map((block: { _type: string }) => {
  switch (block._type) {
    case "blockHero": return <Hero {...block} />;
    case "blockText": return <TextBlock {...block} />;
    default: return null;
  }
})}
